YII_ENV = dev
YII_DEBUG = true
YII_ENABLE_ERROR_HANDLER = false

DB_CLASS = yii\db\Connection
DB_HOST = localhost
DB_PORT = 3306
DB_NAME = app_db
DB_USERNAME = root
DB_PASSWORD =
DB_TABLE_PREFIX =
DB_CHARSET = utf8
DB_DSN = "mysql:host=${DB_HOST};port=${DB_PORT};dbname=${DB_NAME}"

APP_MIGRATION_LOOKUP = @console/migrations,@vendor/dektrium/yii2-user/migrations,@yii/rbac/migrations
APP_ADMINS = admin
APP_ADMIN_EMAIL = admin@example.com

FRONT_APP_NAME = "Awesome application"
FRONT_APP_LANGUAGES = uk,ru,en
FRONT_COOKIE_KEY = <cookie_validation_key>

BACK_APP_NAME = ${FRONT_APP_NAME}
BACK_APP_LANGUAGES = ${FRONT_APP_LANGUAGES}
BACK_COOKIE_KEY = <cookie_validation_key>
